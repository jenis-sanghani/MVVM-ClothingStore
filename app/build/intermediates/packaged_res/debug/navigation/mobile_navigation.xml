<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.designer.fashion.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_category"
        android:name="com.designer.fashion.ui.category.CategoryFragment"
        android:label="@string/title_category"
        tools:layout="@layout/fragment_category" />

    <fragment
        android:id="@+id/navigation_curate"
        android:name="com.designer.fashion.ui.curate.CurateFragment"
        android:label="@string/title_curate"
        tools:layout="@layout/fragment_curate" />

    <fragment
        android:id="@+id/navigation_sale"
        android:name="com.designer.fashion.ui.more.MoreFragment"
        android:label="@string/title_sale"
        tools:layout="@layout/fragment_category" />

    <fragment
        android:id="@+id/navigation_more"
        android:name="com.designer.fashion.ui.more.MoreFragment"
        android:label="@string/title_more"
        tools:layout="@layout/fragment_curate" />
</navigation>